            "weight": 1
          },
          "rotate": {
            "type": "RandomRotate",
            "kwargs": {
              "degrees": [-5, 5]
            },
            "weight": 1
          }
        },
        "wrist_tfs": {
          "hue": {
            "type": "ColorJitter",
            "kwargs": {
              "hue": [-0.05, 0.05]
            },
            "weight": 1
          },
          "contrast": {
            "type": "ColorJitter",
            "kwargs": {
              "contrast": [0.8, 1.2]
            },
            "weight": 1
          },
          "sharpness": {
            "type": "SharpnessJitter",
            "kwargs": {
              "sharpness": [0.5, 1.5]
            },
            "weight": 1
          },
          "brightness": {
            "type": "ColorJitter",
            "kwargs": {
              "brightness": [0.8, 1.2]
            },
            "weight": 1
          },
          "saturation": {
            "type": "ColorJitter",
            "kwargs": {
              "saturation": [0.5, 1.5]
            },
            "weight": 1
          }
        },
        "random_order": false,
        "max_num_transforms": 3
      },
      "use_imagenet_stats": true
