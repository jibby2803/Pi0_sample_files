      "video_backend": "torchcodec",
      "image_transforms": {
        "enable": true,
        "image_tfs": {
          "hue": {
            "type": "ColorJitter",
            "kwargs": {
              "hue": [-0.05, 0.05]
            },
            "weight": 1
          },
          "contrast": {
            "type": "ColorJitter",
            "kwargs": {
              "contrast": [0.8, 1.2]
            },
            "weight": 1
          },
          "sharpness": {
            "type": "SharpnessJitter",
